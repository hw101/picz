{
  "name": "picz",
  "private": true,
  "scripts": {
    "build": "ionic-app-scripts build --dev",
    "build-prod": "ionic-app-scripts build --prod",
    "gulp-build": "gulp build",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "cleanwww": "rm -rf ./www/*",
    "prebuild": "ionic2-custom-icons",
    "prewatch": "ionic2-custom-icons",
    "bundle-ios": "npm run gulp-build && npm run cleanwww && ionic state restore --plugins && npm run build-prod && ionic run ios --device",
    "bundle-android": "npm run gulp-build && npm run cleanwww && ionic state restore --plugins && npm run build-prod && ionic run android"
  },
  "config": {
    "ionic_rollup": "./rollup.config.js",
    "ionic_copy": "./copy.config.js",
    "ionic_sass": "./sass.config.js",
    "ionic_uglifyjs": "./uglifyjs.config.js",
    "ionic_cleancss": "./cleancss.config.js",
    "custom_icons": "./custom-icons.config.js",
    "ionic_debug_mode": "false"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/compiler-cli": "0.6.2",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@ionic/storage": "1.0.3",
    "angular2-moment": "^1.0.0-beta.5",
    "global": "^4.3.1",
    "ionic-angular": "2.0.0-rc.1",
    "ionic-native": "2.2.3",
    "ionic2-custom-icons": "^0.3.0",
    "ionicons": "3.0.0",
    "lodash.flatten": "^4.4.0",
    "moment": "^2.15.1",
    "process": "^0.11.9",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^0.0.36",
    "babel-core": "^6.17.0",
    "babel-helpers": "^6.16.0",
    "babel-plugin-angular2-annotations": "^5.1.0",
    "babel-plugin-class-name": "github:zeddidragon/babel-plugin-class-name",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "gulp-pug": "^3.1.0",
    "gulp-watch": "^4.3.10",
    "pluralize": "^3.0.0",
    "require-tree": "^1.1.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.0.9",
    "through2": "^2.0.1",
    "typescript": "^2.0.3"
  },
  "description": "picz: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": []
}
